
config ENABLE_QMC5883L_SENSOR
  bool "enable magnetometer"
  default y
  help
    enable or disable sensor module
config QMC5883L_RANGE
    int "enter range"
    default 2000
    help
      Magnetometer full-scale range
      Valid values: 2000 or 8000 (Gauss).

config QMC5883L_ODR
    int "Output data rate (Hz)"
	  default 50
	  help
	    Magnetometer output data rate in Hz.
	    Typical values: 10, 50, 100, 200.

config QMC5883L_OSR
    int "Over-sample ratio"
	  default 128
	  help
	    Magnetometer over-sampling ratio.
	    Typical values: 64, 128, 256, 512.